            <TextBox x:Name="city" HorizontalAlignment="Left" Margin="127,110,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Background="#066a3f" Foreground="White" FontFamily="cambria"/>
            <TextBox x:Name="email" HorizontalAlignment="Left" Margin="127,133,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Background="#066a3f" Foreground="White" FontFamily="cambria"/>
            <TextBox x:Name="phoneNumber" HorizontalAlignment="Left" Margin="127,156,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Background="#066a3f" Foreground="White" FontFamily="cambria"/>
            <Button Content="Register" HorizontalAlignment="Left" Margin="198,202,0,0" VerticalAlignment="Top" Width="49" Click="Button_Click" RenderTransformOrigin="-0.136,0.37" Background="#066a3f" Foreground="White" FontFamily="cambria"/>

        </Grid>

    </Grid>
