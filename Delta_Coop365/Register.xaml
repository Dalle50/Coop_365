        xmlns:local="clr-namespace:Delta_Coop365"
        mc:Ignorable="d"
        Title="Register" Height="299" Width="413">
    <Grid Margin="0,0,511,181">

        <Grid Margin="42,18,-332,-169">
            <TextBlock HorizontalAlignment="Left" Margin="73,42,0,0" TextWrapping="Wrap" Text="Navn" VerticalAlignment="Top"/>
            <TextBlock HorizontalAlignment="Left" Margin="73,63,0,0" TextWrapping="Wrap" Text="Adresse" VerticalAlignment="Top"/>
            <TextBlock HorizontalAlignment="Left" Margin="73,87,0,0" TextWrapping="Wrap" Text="Postnr" VerticalAlignment="Top" RenderTransformOrigin="1.17,1.147"/>
            <TextBlock HorizontalAlignment="Left" Margin="73,112,0,0" TextWrapping="Wrap" Text="By" VerticalAlignment="Top" RenderTransformOrigin="1.245,1.372"/>
            <TextBlock HorizontalAlignment="Left" Margin="73,135,0,0" TextWrapping="Wrap" Text="Email" VerticalAlignment="Top" RenderTransformOrigin="1.145,1.443"/>
